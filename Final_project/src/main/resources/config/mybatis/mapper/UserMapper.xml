<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="three.team.mapper.UserMapper">
  	<insert id="join">
  		insert into project_user values(#{userNum} ,#{userId}, #{userPassword}, #{userName}, #{userNick}, #{userEmail}, #{userAddr1}, #{userAddr2}, #{userAddr3}, 0, 100000, #{userBankName}, #{userBankAccount}, #{userBronzeBadge}, #{userSilverBadge}, #{userGoldBadge} )

  	</insert>
  </mapper>